config_version=5

[application]

config/name="ShroomGameReal"
run/main_scene="res://scenes/default_game.tscn"
config/features=PackedStringArray("4.5", "C#", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

MouseReleaserSingleton="*res://Utilities/MouseReleaser Singleton.tscn"
SettingsManagerSingleton="*res://addons/settings_helper/SettingsManager Singleton.tscn"
GlobalGameStateSingleton="*res://Singletons/Global Game State Singleton.tscn"
MusicManagerSingleton="*res://Music/Music Manager Singleton.tscn"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080

[dotnet]

project/assembly_name="ShroomGameReal"

[editor_plugins]

enabled=PackedStringArray("res://addons/light_shafts/plugin.cfg", "res://addons/settings_helper/plugin.cfg")

[file_customization]

folder_colors={
"res://Music/": "pink",
"res://addons/": "gray",
"res://scenes/": "green"
}

[filesystem]

import/blender/enabled=false

